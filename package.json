      "url": "https://github.com/tjwebb"
    }
  ],
  "dependencies": {
    "call": "^5",
    "joi": "^14.3.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@fabrix/fabrix": "^1.6.4",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@types/lodash": "^4.14.109",
