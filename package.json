    "smokesignals": "^3",
    "source-map-support": "^0.5.13",
    "standard-version": "^7.0.0",
    "tslib": "~1.9.0",
    "tslint": "~5.14.0",
    "tslint-microsoft-contrib": "~6.1.0",
    "tsutils": "~3.9.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.7",
    "typescript": "~3.3.4000"
  },
  "peerDependencies": {
    "@fabrix/fabrix": "^1.6.0"
